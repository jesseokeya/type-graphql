{
  "name": "type-graphql",
  "version": "1.0.0",
  "description": "Playing around with apollo graphql type-graphql typorm and redis",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon ts-node-dev --respawn --ignore-watch node_modules -- src/index.ts",
    "dev:debug": "nodemon -x ts-node-dev --respawn --debug --ignore-watch node_modules -- src/index.ts",
    "db:setup": "ts-node ./src/test-utils/setup.ts",
    "test": "npm run db:setup && jest",
    "prod": "npm run build && cross-env NODE_ENV=production node ./dist/index.js",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesseokeya/type-graphql.git"
  },
  "keywords": [
    "apollo-server",
    "type-graphql",
    "typorm",
    "and",
    "redis"
  ],
  "author": "Jesse Okeya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesseokeya/type-graphql/issues"
  },
  "homepage": "https://github.com/jesseokeya/type-graphql#readme",
  "dependencies": {
    "add": "^2.0.6",
    "apollo-server-express": "^2.16.1",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "ioredis": "^4.17.3",
    "nodemailer": "^6.4.11",
    "pg": "^8.3.2",
    "pino": "^6.5.1",
    "pino-pretty": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0-rc.3",
    "typeorm": "^0.2.25",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-redis": "^0.0.14",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^7.14.4",
    "@types/pino": "^6.3.0",
    "@types/uuid": "^8.3.0",
    "jest": "^26.4.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.58",
    "typescript": "^3.9.7"
  },
  "private": true
}
